class taskmanager
   def_init_(self):
        self.tasks={}
def add_tasks(name,id,status,dta):
    dta[name].append({id,status})
    
def update_taks(name,id,status):
    if name in self.tasks:
        if id 
        self.tasks[name]['id']=[id]
        if status
        self.tasks[name]['status']=[status]
        print("task (name) updated sucessfully")
        else
        print("task not found")
    
def deleting_tasks(self,name):
    if name in self.tasks:
        del self.tasks[name]
         else
        print("task not found")
